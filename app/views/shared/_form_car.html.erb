<%= simple_form_for @car do |f|%>
<%= f.input :brand  %>
<%= f.input :model  %>
<%= f.input :year  %>
<%= f.input :km  %>
<%= f.input :daily_rate  %>
<%= f.input :city  %>
<%= f.input :category, collection: [ "Vintage", "Luxury", "Off-road", "Sport"] %>
<%= f.submit class:"btn btn-primary" %>
<% end %>

